{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "properties": {
    "beaver": {
      "properties": {
        "debug": {
          "type": "boolean"
        },
        "howlite": {
          "properties": {
            "caldav": {
              "properties": {
                "calendar": {
                  "type": "string"
                },
                "host": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                },
                "path": {
                  "type": "string"
                },
                "port": {
                  "type": "integer"
                },
                "scheme": {
                  "type": "string"
                },
                "user": {
                  "type": "string"
                }
              },
              "required": ["scheme", "host", "user", "password", "calendar"],
              "type": "object"
            }
          },
          "required": ["caldav"],
          "type": "object"
        },
        "sapphire": {
          "properties": {
            "sql": {
              "properties": {
                "host": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                },
                "port": {
                  "type": "integer"
                }
              },
              "required": ["host", "port", "password"],
              "type": "object"
            }
          },
          "required": ["sql"],
          "type": "object"
        },
        "server": {
          "properties": {
            "port": {
              "type": "integer"
            }
          },
          "required": ["port"],
          "type": "object"
        }
      },
      "required": ["server", "howlite", "sapphire", "debug"],
      "type": "object"
    },
    "common": {
      "properties": {
        "metadata": {
          "type": "object"
        }
      },
      "type": "object"
    },
    "configMap": {
      "properties": {
        "metadata": {
          "type": "object"
        }
      },
      "type": "object"
    },
    "container": {
      "properties": {
        "spec": {
          "type": "object"
        }
      },
      "type": "object"
    },
    "deployment": {
      "properties": {
        "metadata": {
          "type": "object"
        },
        "spec": {
          "type": "object"
        }
      },
      "type": "object"
    },
    "name": {
      "type": "string"
    },
    "pod": {
      "properties": {
        "metadata": {
          "type": "object"
        },
        "spec": {
          "type": "object"
        }
      },
      "type": "object"
    },
    "secret": {
      "properties": {
        "metadata": {
          "type": "object"
        }
      },
      "type": "object"
    },
    "service": {
      "properties": {
        "metadata": {
          "type": "object"
        },
        "port": {
          "type": "integer"
        },
        "spec": {
          "type": "object"
        }
      },
      "required": ["port"],
      "type": "object"
    }
  },
  "required": ["beaver", "service"],
  "type": "object"
}
