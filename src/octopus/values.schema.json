{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "properties": {
    "common": {
      "properties": {
        "metadata": {
          "type": "object"
        }
      },
      "type": "object"
    },
    "configMap": {
      "properties": {
        "metadata": {
          "type": "object"
        }
      },
      "type": "object"
    },
    "container": {
      "properties": {
        "spec": {
          "type": "object"
        }
      },
      "type": "object"
    },
    "deployment": {
      "properties": {
        "metadata": {
          "type": "object"
        },
        "spec": {
          "type": "object"
        }
      },
      "type": "object"
    },
    "name": {
      "type": "string"
    },
    "octopus": {
      "properties": {
        "beaver": {
          "properties": {
            "http": {
              "properties": {
                "host": {
                  "type": "string"
                },
                "path": {
                  "type": "string"
                },
                "port": {
                  "type": "integer"
                },
                "scheme": {
                  "type": "string"
                }
              },
              "required": ["scheme", "host"],
              "type": "object"
            }
          },
          "required": ["http"],
          "type": "object"
        },
        "debug": {
          "type": "boolean"
        },
        "dingo": {
          "properties": {
            "srt": {
              "properties": {
                "host": {
                  "type": "string"
                },
                "port": {
                  "type": "integer"
                }
              },
              "required": ["host", "port"],
              "type": "object"
            }
          },
          "required": ["srt"],
          "type": "object"
        },
        "gecko": {
          "properties": {
            "http": {
              "properties": {
                "host": {
                  "type": "string"
                },
                "path": {
                  "type": "string"
                },
                "port": {
                  "type": "integer"
                },
                "scheme": {
                  "type": "string"
                }
              },
              "required": ["scheme", "host"],
              "type": "object"
            }
          },
          "required": ["http"],
          "type": "object"
        },
        "server": {
          "properties": {
            "ports": {
              "properties": {
                "http": {
                  "type": "integer"
                },
                "srt": {
                  "type": "integer"
                }
              },
              "required": ["http", "srt"],
              "type": "object"
            }
          },
          "required": ["ports"],
          "type": "object"
        },
        "stream": {
          "properties": {
            "timeout": {
              "type": "string"
            },
            "window": {
              "type": "string"
            }
          },
          "required": ["timeout", "window"],
          "type": "object"
        }
      },
      "required": ["server", "stream", "beaver", "dingo", "gecko", "debug"],
      "type": "object"
    },
    "pod": {
      "properties": {
        "metadata": {
          "type": "object"
        },
        "spec": {
          "type": "object"
        }
      },
      "type": "object"
    },
    "service": {
      "properties": {
        "metadata": {
          "type": "object"
        },
        "ports": {
          "properties": {
            "http": {
              "type": "integer"
            },
            "srt": {
              "type": "integer"
            }
          },
          "required": ["http", "srt"],
          "type": "object"
        },
        "spec": {
          "type": "object"
        }
      },
      "required": ["ports"],
      "type": "object"
    }
  },
  "required": ["octopus", "service"],
  "type": "object"
}
